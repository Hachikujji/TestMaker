<Window
    x:Class="TestMaker.Views.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:common="clr-namespace:TestMaker.Stuff;assembly=TestMaker.Stuff"
    xmlns:prism="http://prismlibrary.com/"
    Title="TestMaker"
    Width="800"
    Height="800"
    MinWidth="800"
    MinHeight="800"
    prism:ViewModelLocator.AutoWireViewModel="True"
    WindowStartupLocation="CenterScreen">
    <Grid>
        <ContentControl prism:RegionManager.RegionName="{x:Static common:StaticProperties.ContentRegion}" />
    </Grid>
</Window>